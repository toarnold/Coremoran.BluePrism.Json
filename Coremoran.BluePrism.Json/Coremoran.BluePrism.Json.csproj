<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Authors>Tobias Arnold</Authors>
    <Description>This library helps to serialize and deserialize JSON data to or from BluePrism collections.</Description>
    <PackageTags>BluePrism Json</PackageTags>
    <Company>toarnold</Company>
    <Nullable>warnings</Nullable>
    <PackageProjectUrl>https://github.com/toarnold/Coremoran.BluePrism.Json</PackageProjectUrl>
    <RepositoryUrl>https://github.com/toarnold/Coremoran.BluePrism.Json</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
  </ItemGroup>

</Project>
